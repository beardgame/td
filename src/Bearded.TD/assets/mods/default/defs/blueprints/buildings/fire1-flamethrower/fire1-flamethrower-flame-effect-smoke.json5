{
  id: "fire1-flamethrower-flame-effect-smoke",
  components: [
    { id: "particles", parameters: { } },
    { id: "particlesMoveLinear" },
    { id: "particlesGravity", parameters: { factor: -0.1 } },
    { id: "particlesDrag", parameters: { linear: 3, angular: 2 } },

    { id: "particlesDraw", parameters: {
      sprite: "default.particle.mote",
      drawMode: "sprite",
      reverseOrder: true
    } },

    { id: "particlesSpawnContinuouslyFromParentProjectiles", parameters: {
      interval: 0.05,
      intervalNoise: 0.2,
      randomStartInterval: true,

      lifeTime: 1,
      lifeTimeNoise: 0.2,

      inheritVelocity: 1,
      velocity: [0, 0, 0.5],
      velocityNoise: 0.1,
      randomVelocity: 0.2,
      randomVelocityNoise: 0.1,
      orientationNoise: 1,
      angularVelocity: 90,
      angularVelocityNoise: 1,
      randomAngularVelocitySign: true,
    } },

    { id: "particlesKeyFrames", parameters: {
      scaleToParticleLifeTime: true,
      animation: {
        keyFrames: [
          { duration: 0.0, size: 0.1, color: "00000000" },
          { duration: 0.5, size: 0.1, color: "00000000" },
          { duration: 0.4, size: 0.5, color: "88000000" },
          { duration: 0.2, size: 0.6, color: "00000000" },
        ],
      }
    } },

    { id: "particlesDraw", parameters: {
      sprite: "default.particle.mote",
      drawMode: "sprite",
      reverseOrder: true,
    } }
  ]
}
