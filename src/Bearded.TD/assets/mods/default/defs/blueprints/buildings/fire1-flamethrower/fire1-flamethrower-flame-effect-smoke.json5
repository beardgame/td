            betweenLastTwoProjectilesIfCreatedWithin: 0.15,
            startDelay: 0.05,

            lifeTime: 0.6,
            lifeTimeNoise: 0.2,

            inheritVelocity: 1.5,
            velocity: [0, 0, 0.5],
            velocityNoise: 0,
            randomVelocity: 0.5,
